<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <managed-bean>
        <managed-bean-name>typeOfMemberMBean</managed-bean-name>
        <managed-bean-class>com.mysweethome.jsfmanagedbean.TypeOfMemberMBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>typeOfEstateMBean</managed-bean-name>
        <managed-bean-class>com.mysweethome.jsfmanagedbean.TypeOfEstateMBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>contactDetailsMBean</managed-bean-name>
        <managed-bean-class>com.mysweethome.jsfmanagedbean.ContactDetailsMBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>

